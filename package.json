{
  "name": "@roo-code/types",
  "version": "1.3.0",
  "description": "TypeScript type definitions for Roo Code",
  "publishConfig": {
    "access": "public"
  },
  "author": "Roo Code Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RooCodeInc/Roo-Code-Types.git"
  },
  "bugs": {
    "url": "https://github.com/RooCodeInc/Roo-Code-Types/issues"
  },
  "homepage": "https://github.com/RooCodeInc/Roo-Code-Types/tree/main/roo-code-types",
  "keywords": [
    "roo",
    "roo-code",
    "ai"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "README.md"
  ],
  "packageManager": "pnpm@10.8.1",
  "scripts": {
    "build": "echo \"No build required - this package only contains type definitions\"",
    "prepublishOnly": "npm run build",
    "check-types": "tsc --noEmit",
    "release:test": "dotenvx run -f .env.local -- semantic-release --dry-run",
    "release": "dotenvx run -f .env.local -- semantic-release --no-ci"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.44.0",
    "semantic-release": "^24.2.3"
  }
}
